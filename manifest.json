{
    "name": "Break from Work Reminder",
    "description": "Build an Extension!",
    "version": "1.0",
    "manifest_version": 3,
    "icons": {
        "128": "logo.jpg"  
    },
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "notifications",
        "storage",
        "alarms",
        "activeTab",
        "webNavigation"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "128": "logo.jpg" 
        }
    }
}
